<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %> <br>
    <%= f.label :password %>
    <%= f.password_field :password%> <br>
    <%= f.label :email %>
    <%= f.email_field :email %> <br>
    <%= f.label :profile_pic %>
    <%= f.text_field :profile_pic %> <br>
    <%= f.label :bio %>
    <%= f.text_field :bio %> <br>
    <%= f.submit "Sign Up" %>
    <% end %>