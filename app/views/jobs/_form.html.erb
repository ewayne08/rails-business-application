<%= render partial: "layouts/error", locals: {object: @job} %>

<%= form_with model: job, url: url, method: method do |f| %>
    <%= f.hidden_field "company_id", value: company_id%>
    <%= f.label "Name" %>
    <%= f.text_field :name %> <br>
    <%= f.label "Category" %> 
    <%= f.text_field :category %> <br>
    <%= f.label "Description" %> 
    <%= f.text_area :description %> <br>
    <%= f.hidden_field :company_id %>
    <%= f.submit %>
<% end %>
